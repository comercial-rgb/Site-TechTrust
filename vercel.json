{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" },
    { "src": "*.{png,webp,ico,jpg,jpeg,svg,css,js}", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.js"
    }
  ]
}