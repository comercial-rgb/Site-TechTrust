    .hero-cta .cta, .hero-cta .cta.secondary {
      min-width:280px;
      justify-content:center;
      text-align:center;
      font-size:clamp(15px,1.1vw,17px);
      padding:16px 24px;
    }
    .nav-links > .cta{
      min-width:200px;
      padding:10px 18px;
      font-size:.9rem;
    }
    .lang{display:flex; gap:10px; align-items:center}
    .lang button{background:transparent; border:1px solid rgba(148,163,184,.3); color:var(--silver); padding:6px 10px; border-radius:999px; cursor:pointer; font-weight:700; font-size:1.2rem; line-height:1; min-width:40px}
