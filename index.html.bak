    function setLang(l){
      document.querySelectorAll('[data-i18n]').forEach(el=>{
        const k = el.getAttribute('data-i18n');
        if(I18N[l] && I18N[l][k]) el.textContent = I18N[l][k];
      });
      document.querySelectorAll('.lang button').forEach(b=>b.classList.toggle('active', b.dataset.lang===l));
      document.documentElement.lang = l;
    }

    document.addEventListener('DOMContentLoaded', ()=>{
      // Debug: verificar se I18N carregou
      console.log('âœ… TechTrust I18N loaded:', Object.keys(I18N));
      
      document.querySelectorAll('.lang button').forEach(btn=>{
        btn.addEventListener('click', ()=> setLang(btn.dataset.lang));
      });
      
      // Force initial language
      const defaultLang = 'en';
      console.log('ðŸŒ Setting language to:', defaultLang);
      setLang(defaultLang);

      // Detect local static preview (no serverless) and warn
      const serverlessWarning = document.createElement('div');
      serverlessWarning.style.cssText = 'background:#1e293b;color:#fbbf24;padding:10px 14px;border:1px solid #fbbf24;border-radius:10px;font-size:13px;margin:12px 0';
      serverlessWarning.textContent = 'Aviso: API indisponÃ­vel em servidor estÃ¡tico local. Use "vercel dev" para testar envio de formulÃ¡rios.';
      fetch('/api/contact', { method:'OPTIONS'}).catch(()=>{
        // If OPTIONS fails quickly, assume no serverless runtime
        document.querySelector('#contact .container')?.prepend(serverlessWarning);
      });

      const form = document.getElementById('contact-form');
      const contactStatus = document.createElement('p');
      contactStatus.style.cssText='font-size:13px;margin:6px 0 0';
      form?.appendChild(contactStatus);
      form?.addEventListener('submit', async (e)=>{
        e.preventDefault();
        contactStatus.textContent='Enviando...'; contactStatus.style.color='#60a5fa';
        const data = Object.fromEntries(new FormData(form).entries());
        try{
          const res = await fetch('/api/contact',{
            method:'POST', headers:{'Content-Type':'application/json'}, body: JSON.stringify(data)
          });
          if(!res.ok) throw new Error('Network response was not ok');
          contactStatus.textContent=`Obrigado! Entraremos em contato em ${data.email}.`; contactStatus.style.color='#38bdf8';
          form.reset();
        }catch(err){
          console.error(err);
          contactStatus.textContent='Falha ao enviar. Verifique sua conexÃ£o ou tente via vercel dev.'; contactStatus.style.color='#f87171';
        }
      })

      // Slider controls
      const slides = document.getElementById('service-slides');
      const prev = document.getElementById('prevSlide');
      const next = document.getElementById('nextSlide');
      const slideWidth = ()=> slides?.querySelector('img')?.getBoundingClientRect().width || 0;
      prev?.addEventListener('click', ()=> slides.scrollBy({left: -slideWidth(), behavior:'smooth'}));
      next?.addEventListener('click', ()=> slides.scrollBy({left: slideWidth(), behavior:'smooth'}));
      // Autoplay com loop
      let autoPlayInt = null;
      function startAutoplay(){
        if(autoPlayInt) clearInterval(autoPlayInt);
        autoPlayInt = setInterval(()=>{
          if(!slides) return;
          const maxScroll = slides.scrollWidth - slides.clientWidth;
          // Se chegou ao final, volta para o inÃ­cio
          if (Math.abs(slides.scrollLeft - maxScroll) < 2 || slides.scrollLeft >= maxScroll - 2) {
            slides.scrollTo({left: 0, behavior: 'smooth'});
          } else {
            slides.scrollBy({left: slideWidth(), behavior:'smooth'});
          }
        }, 3500);
      }
      function stopAutoplay(){
        if(autoPlayInt) clearInterval(autoPlayInt);
      }
      slides?.addEventListener('mouseenter', stopAutoplay);
